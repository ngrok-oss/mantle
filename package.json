    "docs:dev": "remix dev --manual",
    "docs:serve": "remix-serve ./build/index.js",
    "fmt:check": "prettier --check .",
    "fmt": "prettier --write .",
    "gen:docs-routes": "tsx scripts/gen-docs-routes",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
