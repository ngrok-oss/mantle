{
  "name": "root",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "browserslist": [
    "last 2 years, not dead, > 0.2%"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "cmd:storybook": "STORYBOOK_DISABLE_TELEMETRY=1 storybook",
    "build-storybook": "pnpm run cmd:storybook build",
    "docs:build": "pnpm run --filter docs build",
    "docs:dev": "pnpm run --filter docs dev",
    "docs:start": "pnpm run --filter docs start",
    "fmt:check": "pnpm run -r --parallel fmt:check",
		"fmt": "pnpm run -r --parallel fmt",
    "lint:js": "pnpm run -r --parallel lint:js",
    "storybook": "pnpm run cmd:storybook dev -p 6006",
    "test": "pnpm run -r --parallel test",
    "typecheck": "pnpm run -r --parallel typecheck"
  },
