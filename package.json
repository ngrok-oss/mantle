{
  "name": "@ngrok/mantle",
  "description": "mantle is ngrok's UI library and design system.",
  "author": "ngrok",
  "license": "MIT",
  "version": "0.0.3",
  "homepage": "https://mantle.ngrok.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngrok-oss/mantle"
  },
  "sideEffects": false,
  "type": "module",
  "browserslist": [
    "last 2 years, not dead, > 0.2%"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./tailwind.preset": {
      "import": "./dist/tailwind.preset.js",
      "default": "./dist/tailwind.preset.js",
      "types": "./dist/tailwind.preset.d.ts"
    },
    "./assets/*": "./dist/assets/*"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build-storybook": "pnpm run cmd:storybook build",
    "cmd:storybook": "STORYBOOK_DISABLE_TELEMETRY=1 storybook",
    "docs:build": "remix build",
    "docs:dev": "remix dev --manual",
    "docs:serve": "remix-serve ./build/index.js",
    "fmt:check": "prettier --cache --check .",
    "fmt": "prettier --write --cache .",
    "gen:remix-routes": "tsx scripts/gen-remix-routes",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepublishOnly": "rm -rf dist && rollup -c",
    "storybook": "pnpm run cmd:storybook dev -p 6006",
    "test": "TZ=UTC vitest",
    "typecheck": "tsc --incremental --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-tooltip": "1.0.7",
    "@remix-run/css-bundle": "2.3.1",
    "@remix-run/node": "2.3.1",
    "@remix-run/react": "2.3.1",
    "@remix-run/serve": "2.3.1",
    "@uidotdev/usehooks": "2.4.1",
    "@vercel/remix": "2.2.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "isbot": "3.7.1",
    "lucide-react": "0.294.0",
    "prismjs": "1.29.0",
    "tailwind-merge": "2.0.0",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "1.0.7",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.5",
    "@babel/preset-env": "7.23.5",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@commander-js/extra-typings": "11.1.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@remix-run/dev": "2.3.1",
    "@remix-run/eslint-config": "2.3.1",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-typescript": "11.1.5",
    "@storybook/addon-essentials": "7.6.2",
    "@storybook/addon-interactions": "7.6.2",
    "@storybook/addon-links": "7.6.2",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/blocks": "7.6.2",
    "@storybook/builder-vite": "7.6.2",
    "@storybook/react": "7.6.2",
    "@storybook/react-vite": "7.6.2",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/lodash.camelcase": "4.3.9",
    "@types/node": "20.10.1",
    "@types/prismjs": "1.26.3",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-react-swc": "3.5.0",
    "@vitest/ui": "0.34.6",
    "autoprefixer": "10.4.16",
    "babel-plugin-dev-expression": "0.2.3",
    "browserslist": "4.22.1",
    "commander": "11.1.0",
    "copyfiles": "2.4.1",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.15",
    "fast-glob": "3.3.2",
    "jsdom": "22.1.0",
    "lodash.camelcase": "4.3.0",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "4.6.1",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-extensions": "0.1.0",
    "storybook": "7.6.2",
    "tsx": "4.6.1",
    "typescript": "5.3.2",
    "vite": "5.0.4",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6",
    "vitest-dom": "0.1.1"
  },
  "peerDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^18",
    "react-dom": "^18"
  }
}