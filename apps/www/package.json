    "@remix-run/serve": "2.15.1",
    "isbot": "5.1.18",
    "prismjs": "1.29.0",
    "react": "18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.28.0",
    "tiny-invariant": "1.3.3"
