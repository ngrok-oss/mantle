{
  "extends": "@cfg/tsconfig/tsconfig.web-app.json",
  "include": ["**/*.ts", "**/*.tsx", "**/*.mdx", ".react-router/types/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./app/*"]
    },
    "types": [
      "@react-router/node",
      "vite/client",
      "@cfg/tsconfig/css.d.ts",
      "@cfg/tsconfig/reset.d.ts",
      "@cfg/tsconfig/svg.d.ts"
    ],
    "customConditions": ["@ngrok/src-custom-condish"],
    "rootDirs": [".", "./.react-router/types"]
  }
}
